{
  "name": "ponzu0529-tools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-full": "yarn build-vue && yarn build-laravel",
    "build-full-dev": "yarn build-vue-dev && yarn build-laravel",
    "build-vue": "vue-cli-service build && yarn build-vue-sass",
    "build-vue-dev": "vue-cli-service build --mode development && yarn build-vue-sass",
    "build-vue-sass": "sass sass/:public/css",
    "build-vue-sass-watch": "sass sass/:public/css --watch",
    "build-laravel": "yarn build-laravel-js && yarn build-laravel-sass",
    "build-laravel-js": "webpack",
    "build-laravel-sass": "sass resources/sass/:dist/css",
    "build-test-ts": "webpack --config webpack.config.test.js",
    "build-mock-sass": "sass sass/:design/css",
    "build-mock-sass-watch": "sass sass/:design/css --watch",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "@types/lodash": "^4.14.172",
    "axios": "^0.21.4",
    "buefy": "^0.9.8",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-head": "^2.2.0",
    "vue-js-modal": "^2.0.1",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-module-decorators": "^2.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.10.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@popperjs/core": "^2.10.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "bootstrap": "^5.1.3",
    "copy-webpack-plugin": "^6",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "laravel-mix": "^6.0.6",
    "lint-staged": "^9.5.0",
    "postcss": "^8.1.14",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "vue-cli-service lint"
  }
}